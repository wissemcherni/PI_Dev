<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="562.0" prefWidth="790.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.CategoryController">
   <children>
      <Label layoutX="354.0" layoutY="9.0" prefHeight="48.0" prefWidth="83.0" text="Category">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="197.0" layoutY="69.0" prefHeight="249.0" prefWidth="397.0" spacing="25.0">
         <children>
            <TextField fx:id="tfcategory" />
            <ComboBox fx:id="_list_caegory_" onAction="#initdata" prefWidth="150.0" />
            <Button fx:id="_btn_" alignment="CENTER" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#addcategory" text="ADD" />
            <Button fx:id="_delete_btn_" alignment="CENTER" contentDisplay="BOTTOM" layoutX="88.0" layoutY="200.0" mnemonicParsing="false" onAction="#delete" text="Supprimer" visible="false" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <VBox layoutX="277.0" layoutY="330.0" prefHeight="200.0" prefWidth="237.0" spacing="25.0">
         <children>
            <Button mnemonicParsing="false" onAction="#produits" prefHeight="55.0" prefWidth="243.0" style="-fx-background-color: red;" text="Gestion Produits" textFill="WHITE">
               <font>
                  <Font name="Arial Black" size="22.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#Category" prefHeight="54.0" prefWidth="245.0" style="-fx-background-color: red;" text="Gestion sous Category" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Button>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#back" prefHeight="48.0" prefWidth="259.0" style="-fx-background-color: red;" text="Retour" textFill="WHITE">
               <font>
                  <Font name="Arial Black" size="15.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
